library(lubridate)
# Set the start and end dates as Date objects
start_date <- as.Date("2024-01-01")
end_date <- as.Date("2024-01-31")
# Generate a sequence of dates between start_date and end_date
date_sequence <- seq(from = start_date, to = end_date, by = "day")
# Randomly sample 100 dates from the date_sequence
random_dates <- sample(date_sequence, 100, replace = FALSE)
random_date <- sample(date_sequence,100, T)
print(random_date)
knitr::kable(random_date)
